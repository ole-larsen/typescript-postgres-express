{
    "name": "vue-express-postgres-starter",
    "version": "0.1.0",
    "description": "A starting point for Node.js express app with TypeScript, Vue JS, Postgres",
    "license": "MIT",
    "scripts": {
        "build-ts": "tsc",
        "build": "npm run build-ts && npm run lint",
        "debug": "npm run build && npm run watch-debug",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "dev": "NODE_ENV=development ts-node api/src/app.main.ts",
        "serve-debug": "nodemon --inspect dist/server.js",
        "serve": "node build/app.main.js",
        "start": "npm run serve",
        "test": "NODE_ENV=test SERVER_PORT=3009 jest --forceExit --verbose --detectOpenHandles",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "watch-node": "nodemon build/app.js",
        "watch-test": "npm run test -- --watchAll",
        "watch-ts": "tsc -w",
        "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "migration:create": "node-pg-migrate create -j sql -m migrations -t __migrations --decamelize true",
        "migration:run": "node-pg-migrate up     -j sql -m migrations -t __migrations --decamelize true",
        "migration:down": "node-pg-migrate down   -j sql -m migrations -t __migrations --decamelize true"
    },
    "dependencies": {
        "@types/bcrypt": "^5.0.0",
        "async": "^3.2.0",
        "bcrypt": "^5.0.1",
        "bluebird": "^3.7.2",
        "body-parser": "1.19.0",
        "class-sanitizer": "^1.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "client-oauth2": "^4.3.3",
        "compression": "1.7.4",
        "cookie-parser": "^1.4.6",
        "dotenv": "8.2.0",
        "errorhandler": "1.5.1",
        "express": "4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.17.2",
        "express-validator": "6.9.2",
        "lodash": "^4.17.21",
        "lusca": "^1.6.1",
        "moment": "^2.29.4",
        "nats": "^2.7.1",
        "node-cron": "^3.0.2",
        "node-fetch": "^2.6.7",
        "node-pg-migrate": "^6.2.1",
        "passport": "^0.6.0",
        "passport-2fa-totp": "^0.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.7.1",
        "prom-client": "^14.0.1",
        "redis": "^4.0.1",
        "reflect-metadata": "^0.1.13",
        "speakeasy": "^2.0.0",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.3.0",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@types/async": "^3.2.5",
        "@types/bluebird": "3.5.33",
        "@types/body-parser": "1.19.0",
        "@types/chai": "4.2.14",
        "@types/compression": "1.7.0",
        "@types/concurrently": "5.2.1",
        "@types/cookie-parser": "^1.4.2",
        "@types/errorhandler": "1.5.0",
        "@types/eslint": "7.2.6",
        "@types/express": "4.17.11",
        "@types/express-flash": "0.0.2",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^26.0.23",
        "@types/jquery": "3.5.5",
        "@types/lodash": "^4.14.170",
        "@types/lusca": "^1.6.2",
        "@types/node": "14.14.25",
        "@types/node-cron": "^3.0.4",
        "@types/node-fetch": "2.5.0",
        "@types/passport": "^1.0.7",
        "@types/passport-local": "^1.0.34",
        "@types/pg": "^8.6.3",
        "@types/request": "2.48.5",
        "@types/request-promise": "4.1.47",
        "@types/shelljs": "0.8.8",
        "@types/speakeasy": "^2.0.7",
        "@types/supertest": "2.0.10",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "4.14.2",
        "@typescript-eslint/parser": "4.14.2",
        "chai": "4.3.0",
        "concurrently": "6.0.2",
        "eslint": "7.19.0",
        "jest": "^27.0.6",
        "nodemon": "^2.0.7",
        "shelljs": "^0.8.5",
        "supertest": "6.1.3",
        "ts-jest": "^27.0.3",
        "ts-node": "9.1.1",
        "typescript": "4.1.3"
    }
}
